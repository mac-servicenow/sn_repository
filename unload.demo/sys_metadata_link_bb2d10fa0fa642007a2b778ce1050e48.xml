        <payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;
&lt;record_update table="sn_onesearch_skins"&gt;
    &lt;sn_onesearch_skins action="INSERT_OR_UPDATE"&gt;
        &lt;description&gt;Fuji-Contextual-Search style&amp;#13;
-Pagination optional, depending on Searcher settings&amp;#13;
-Collapsible optional, depending on Searcher settings&lt;/description&gt;
        &lt;name&gt;Fujified&lt;/name&gt;
        &lt;sys_created_by&gt;ben.hollifield@snc_SHARE&lt;/sys_created_by&gt;
        &lt;sys_created_on&gt;2015-09-20 03:39:29&lt;/sys_created_on&gt;
        &lt;sys_id&gt;d28df12a0f6642007a2b778ce1050e75&lt;/sys_id&gt;
        &lt;sys_mod_count&gt;58&lt;/sys_mod_count&gt;
        &lt;sys_updated_by&gt;ben.hollifield@snc_SHARE&lt;/sys_updated_by&gt;
        &lt;sys_updated_on&gt;2015-09-22 20:00:03&lt;/sys_updated_on&gt;
        &lt;xml&gt;&lt;![CDATA[&lt;section ng-show="fieldPopped"&gt;
	&lt;!--Client scripts rendering--&gt;
	&lt;div ng-repeat="clientScript in clientScripts track by $index"&gt;
		&lt;div ng-bind-html="renderHTML(clientScript)"&gt;&lt;/div&gt;
	&lt;/div&gt;
	&lt;!--Collapser rendering--&gt;
	&lt;div id="collapserFujified" ng-show="collapsible=='true'"&gt;
		&lt;table class="fujified_header"&gt;
        	&lt;tbody&gt;
            	&lt;tr&gt;
                	&lt;td class="fujified_hr"&gt;&lt;hr /&gt;&lt;/td&gt;
                		&lt;td class="fujified_header_button"&gt;
							&lt;button type="button" class="btn btn-default" ng-click="toggleCollapse(true)"&gt;Related Search Results&amp;#160;&lt;span ng-class="{'icon-chevron-right':collapsed=='true','icon-chevron-down':collapsed!='true'}" aria-hidden="true"&gt;&lt;/span&gt;&lt;/button&gt;
                        &lt;/td&gt;
					&lt;td class="fujified_hr"&gt;&lt;hr /&gt;&lt;/td&gt;
				&lt;/tr&gt;
			&lt;/tbody&gt;
		&lt;/table&gt;
	&lt;/div&gt;
	&lt;div id="collapsible" ng-class="{'collapse':collapsed=='true','collapse in':collapsed!='true'}"&gt;
		&lt;!--Search sections rendering--&gt;
		&lt;div&gt;
		&lt;table class="fujified_header"&gt;
        	&lt;tbody&gt;
            	&lt;tr&gt;
                	&lt;td class="fujified_hr"&gt;&lt;hr /&gt;&lt;/td&gt;
                		&lt;td ng-repeat="section in sections" ng-show="!hideSection(sectionResultCounts[$index], section)" class="fujified_header_button"&gt;
							&lt;button ng-class="{active:sectionSelected(section)}" type="button" class="btn btn-default" ng-click="selectSection(section)"&gt;{{section}}&amp;#160;&lt;span class="badge fujified_badge" ng-show="countBadge"&gt;&lt;div ng-bind-html="renderHTML(sectionResultCounts[$index])"&gt;&lt;/div&gt;&lt;/span&gt;&lt;/button&gt;
                        &lt;/td&gt;
					&lt;td class="fujified_hr"&gt;&lt;hr /&gt;&lt;/td&gt;
				&lt;/tr&gt;
			&lt;/tbody&gt;
		&lt;/table&gt;
	&lt;/div&gt;
		&lt;div ng-show="sectionSelected(section) &amp;amp;&amp;amp; !hideSection(sectionResultCounts[$index], section)" ng-repeat="section in sections"&gt;
			&lt;table style="display: table;" class="table table-hover"&gt;
				&lt;tbody&gt;
					&lt;tr ng-repeat="search_link in search_results[section].slice(((currentPage-1)*itemsPerPage), ((currentPage)*itemsPerPage))" class="list_row os-animate" width="100%"&gt;
        				&lt;td class="fujified_row"&gt;
							&lt;div ng-bind-html="renderHTML(search_link)"&gt;&lt;/div&gt;
						&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;!--Paginator rendering--&gt;
			&lt;div ng-show="paginate == 'true' &amp;amp;&amp;amp; search_results[section].length &gt; itemsPerPage" class="text-center"&gt;
				&lt;pagination
					ng-model="currentPage"
					total-items="search_results[section].length"
					items-per-page="itemsPerPage"
					class="pagination-sm"
					previous-text="&amp;#8249;"
					next-text="&amp;#8250;"&gt;
	    		&lt;/pagination&gt;
			&lt;/div&gt;
		&lt;/div&gt;
		&lt;!-- Copy Link action (not offered for forms) --&gt;
		&lt;div class="col-xs-12" style="text-align: center;"&gt;
			&lt;a ng-show="searcherType!='form'" ng-click="aPrompt('Because of a browser limitation the URL can not be placed directly in the clipboard.  Please use Ctrl-C to copy the data and escape to dismiss this dialog', getLink())"&gt;Copy link&lt;/a&gt;
		&lt;/div&gt;
	&lt;/div&gt;
&lt;/section&gt;]]&gt;&lt;/xml&gt;
    &lt;/sn_onesearch_skins&gt;
&lt;/record_update&gt;
</payload>
