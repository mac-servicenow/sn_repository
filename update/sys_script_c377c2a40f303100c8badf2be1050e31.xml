        <script><![CDATA[//Abort submission for duplicate searchers
function onBefore(current, previous) {
	var searcher = new GlideRecord("sn_onesearch_searchers");
	searcher.addQuery("type", "form");
	searcher.addQuery("active", true);
	searcher.addQuery("form", current.form);
	searcher.addQuery("sys_id", "!=", current.sys_id);
	searcher.query();
	if (searcher.next()) {
		gs.addErrorMessage('An active searcher already exists for the ' + current.form.getDisplayValue() + ' form. Aborting.');
		current.setAbortAction(true);
	}
}]]></script>
