        <sys_ui_annotation action="INSERT_OR_UPDATE">
            <is_plain_text>false</is_plain_text>
            <sys_created_by>ben.hollifield_SHARE</sys_created_by>
            <sys_created_on>2015-09-19 23:23:10</sys_created_on>
